{
  "name": "scottboms/logger",
  "description": "Tiny logging helper for Kirby plugins and sites",
  "type": "kirby-plugin",
  "homepage": "https://github.com/scottboms/kirby-logger",
  "license": "MIT",
  "authors": [
    {
      "name": "Scott Boms",
      "email": "plugins@scottboms.com",
      "homepage": "https://scottboms.com"
    }
  ],
  "support": {
    "docs": "https://github.com/scottboms/kirby-logger/blob/main/README.md",
    "source": "https://github.com/scottboms/kirby-logger"
  },
  "keywords": [
    "kirby",
    "kirby-cms",
    "kirby-plugin",
    "kirby4",
    "kirby5",
    "kirby4-plugin",
    "kirby5-plugin",
    "debugging",
    "log"
  ],
  "autoload": {
    "psr-4": {
      "Scottboms\\Logger\\": "classes/"
    }
  },
  "require": {
    "php": ">=8.1.0",
    "getkirby/composer-installer": "^1.1"
  },
  "extra": {
    "installer-name": "kirby-logger",
    "kirby-cms-path": "kirby"
  },
  "config": {
    "optimize-autoloader": true,
    "allow-plugins": {
      "getkirby/composer-installer": true
    }
  }
}
